#include <stdio.h>
int main()
{
	puts("Geeksfor");
	puts("Geeks");

	getchar();
	return 0;
}
















#include <stdio.h>
int main()
{
	fputs("Geeksfor", stdout);
	fputs("Geeks", stdout);

	getchar();
	return 0;
}















#include <stdio.h>
int main()
{
	printf("Geek%sforGeek%s");
	getchar();
	return 0;
}













// C program to show the use of puts
#include <stdio.h>
int main()
{
	puts("Geek%sforGeek%s");
	getchar();
	return 0;
}
