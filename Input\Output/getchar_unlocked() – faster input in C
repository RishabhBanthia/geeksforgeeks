#include <stdio.h>
int main()
{
	char c = getchar_unlocked();
	printf("Entered character is %c", c);
	
	return 0;
}












// A simple C program to demonstrate
// working of putchar_unlocked()
#include <stdio.h>
int main()
{
	// Syntax is same as getchar()
	char c = getchar_unlocked();

	putchar_unlocked(c);

	return 0;
}
