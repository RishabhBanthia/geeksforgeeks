#include <stdio.h>
int main()
{
    printf("%c", getc(stdin));
    return(0);
}














#include <stdio.h>
int main()
{
printf("%c", getchar());
return 0;
}













#include <stdio.h>
#include <conio.h>
int main()
{
printf("%c", getch());
return 0;
}















#include <stdio.h>
#include <conio.h>
// Example for getche() in C
int main()
{
printf("%c", getche());
return 0;
}
