#include<stdio.h>
int main()
{
	char str[80], ch;
	
	scanf("%s", str);
	
	ch = getchar();
	
	printf("%s\n", str);
	printf("%c", ch);
	
	return 0;
}














#include<iostream>
#include<vector>
using namespace std;

int main()
{
	int a;
	char ch[80];
	
	cin >> a;
	cin.getline(ch,80);
	
	cout << a << endl;
	cout << ch << endl;
	
	return 0;
}

















#include<stdio.h>

int main()
{
	char str[80], ch;
	
	scanf("%s", str);
	while ((getchar()) != '\n');
	
	ch = getchar();
	printf("%s\n", str);
	printf("%c", ch);

	return 0;
}


















#include<iostream>

#include<ios>	

#include<limits>
using namespace std;

int main()
{
	int a;
	char str[80];
	
	cin >> a;
	cin.ignore(numeric_limits<streamsize>::max(),'\n');
	cin.getline(str, 80);
	
	cout << a << endl;
	cout << str << endl;

	return 0;
}





#include<iostream>
#include<ios>	
#include<limits>
using namespace std;

int main()
{
	int a;
	char str[80];
	
	cin >> a;
	
	cin.sync();
	cin.getline(str, 80);
	
	cout << a << endl;
	cout << str << endl;

	return 0;
}





#include<iostream>
#include<vector>
using namespace std;

int main()
{
	int a;
	string s;
	
	cin >> a;
	cin >> ws;

	getline(cin, s);

	cout << a << endl;
	cout << s << endl;

	return 0;
}
