#include <stdio.h>
int main()
{
	printf("%lu\n", sizeof(char));
	printf("%lu\n", sizeof(int));
	printf("%lu\n", sizeof(float));
	printf("%lu", sizeof(double));
	return 0;
}





#include <iostream>
using namespace std;

int main()
{
	cout << sizeof(char)<<"\n";
	cout << sizeof(int)<<"\n";
	cout << sizeof(float)<<"\n";
	cout << sizeof(double)<<"\n";
	return 0;
}
