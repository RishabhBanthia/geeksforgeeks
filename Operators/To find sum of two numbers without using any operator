#include <stdio.h>

int add(int x, int y)
{
	return printf("%*c%*c", x, '\r', y, '\r');
}

// Driver code
int main()
{
	printf("Sum = %d", add(3, 4));
	return 0;
}






#include <iostream>
using namespace std;

int add(int x, int y)
{
	return printf("%*c%*c", x, '\r', y, '\r');
}

// Driver code
int main()
{
	printf("Sum = %d", add(3, 4));
	return 0;
}

// This code is contributed by shubhamsingh10







#include <stdio.h>

int main()
{
	int a = 10, b = 5;
	if (b > 0) {
		while (b > 0) {
			a++;
			b--;
		}
	}
	if (b < 0) { // when 'b' is negative
		while (b < 0) {
			a--;
			b++;
		}
	}
	printf("Sum = %d", a);
	return 0;
}

// This code is contributed by Abhijeet Soni
