// CPP program to illustrate (#) operator
#include <stdio.h>
#define mkstr(s) #s
int main(void)
{
	printf(mkstr(geeksforgeeks));
	return 0;
}







// CPP program to illustrate (#) operator
#include <iostream>
using namespace std;

#define a 8.3297

int main()
{
	cout << "Value of a is " << a << endl;

	return 0;
}






// CPP program to illustrate (#) operator
#include <iostream>
using namespace std;

#define MAX(i, j) (((i) > (j)) ? i : j)

int main()
{
	int a, b;

	a = 250;
	b = 25;

	cout << "The maximum is " << MAX(a, b) << endl;

	return 0;
}
