#include <stdio.h>

int x = 0;
int f1()
{
	x = 5;
	return x;
}
int f2()
{
	x = 10;
	return x;
}
int main()
{
	int p = f1() + f2();
	printf("%d ", x);
	return 0;
}











#include <stdio.h>
int main()
{
	int a;
	a = 1, 2, 3;
	printf("%d", a);
	return 0;
}










#include <stdio.h>
int main()
{
	int a = 10, b = 20, c = 30;

	// (c > b > a) is treated as ((c > b) > a), associativity of '>'
	// is left to right. Therefore the value becomes ((30 > 20) > 10)
	// which becomes (1 > 20)
	if (c > b > a)
		printf("TRUE");
	else
		printf("FALSE");
	return 0;
}
