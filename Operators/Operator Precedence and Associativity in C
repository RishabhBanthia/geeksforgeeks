// Associativity is not used in the below program.
// Output is compiler dependent.

#include <stdio.h>

int x = 0;
int f1()
{
	x = 5;
	return x;
}
int f2()
{
	x = 10;
	return x;
}
int main()
{
	int p = f1() + f2();
	printf("%d ", x);
	return 0;
}






#include <stdio.h>
int main()
{
	int a;
	a = 1, 2, 3; // Evaluated as (a = 1), 2, 3
	printf("%d", a);
	return 0;
}
