#include <stdio.h>
int main()
{
	int x = 10;
	int y = 15;

	printf("%d", (x, y));
	getchar();
	return 0;
}













#include <stdio.h>
int main()
{
	int x = 10;
	int y = (x++, ++x);
	printf("%d", y);
	getchar();
	return 0;
}













#include <stdio.h>
int main()
{
	int x = 10, y;
	y = (x++,
		printf("x = %d\n", x),
		++x,
		printf("x = %d\n", x),
		x++);
	printf("y = %d\n", y);
	printf("x = %d\n", x);

	return 0;
}












#include <iostream>

using namespace std;

int main()

{

	int a = 5;

	a = 2, 3, 4;

	cout << a;

	return 0;
}






#include <iostream>
using namespace std;
int main()
{
	cout << "First Line\n",
		cout << "Second Line\n",
		cout << "Third Line\n",
		cout << "Last line";
	return 0;
}
