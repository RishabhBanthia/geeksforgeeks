#include <stdio.h>
int main()
{
int a = 3, b = -8, c = 2;
printf("%d", a % b / c);
return 0;
}





#include <iostream>
using namespace std;

int main() {

int a = 3, b = -8, c = 2;
cout << a % b / c;
return 0;
}





#include <stdio.h>
int main()
{
// a positive and b negative.
int a = 3, b = -8;
printf("%d", a % b);
return 0;
}





#include <stdio.h>
int main()
{
// a negative and b positive
int a = -3, b = 8;
printf("%d", a % b);
return 0;
}





#include <iostream>
using namespace std;

int main()
{
// a negative and b positive
int a = -3, b = 8;
cout << a%b;
return 0;
}





#include <stdio.h>
int main()
{
// a and b both negative
int a = -3, b = -8;
printf("%d", a % b);
return 0;
}





#include <iostream>
using namespace std;

int main() {
// a and b both negative
int a = -3, b = -8;
cout << a % b;
return 0;
}
