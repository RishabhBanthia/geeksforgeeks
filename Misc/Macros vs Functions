#include<iostream>
#define NUMBER 10
using namespace std;

int main()
{
	cout<<NUMBER;
	return 0;
}












#include<stdio.h>
#define NUMBER 10

int main()
{
	printf("%d", NUMBER);
	return 0;
}










#include<stdio.h>
int number()
{
	return 10;
}
int main()
{
	printf("%d", number());
	return 0;
}









#include<iostream>
using namespace std;
int number()
{
	return 10;
}
int main()
{
	cout<<number();
	return 0;
}









#include<stdio.h>
#define CUBE(b) b*b*b
int main()
{
	printf("%d", CUBE(1+2));
	return 0;
}









#include<stdio.h>
int cube(int a)
{
	return a*a*a;
}
int main()
{
	printf("%d", cube(1+2));
	return 0;
}
