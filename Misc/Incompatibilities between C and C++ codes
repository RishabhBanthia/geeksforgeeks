#include<stdio.h>
void fun(a, b)int a;int b;
{
	printf("%d %d", a, b);
}

// Driver code
int main()
{
	fun(8, 9);
	return 0;
}














#include<stdio.h>
#include<malloc.h>
void f(int n)
{
	int* p = malloc(n* sizeof(int));
}

// Driver code
int main()
{
	f(7);
	return 0;
}















#include<stdio.h>
int main()
{
	char array[5] = "Geeks";	

	printf("%s", array);
	return 0;
}















// In C, empty brackets mean any number
// of arguments can be passed
#include<stdio.h>

void fun() { }
int main(void)
{
	fun(10, "GfG", "GQ");
	return 0;
}
