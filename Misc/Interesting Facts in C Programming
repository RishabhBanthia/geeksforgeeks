// C program to demonstrate that arr[0] and
// 0[arr]
#include<stdio.h>
int main()
{
	int arr[10];
	arr[0] = 1;
	printf("%d", 0[arr] );
	
	return 0;	
}







#include<stdio.h>
int main()
<%
	int arr <:10:>;
	arr<:0:> = 1;
	printf("%d", arr<:0:>);

	return 0;
%>









#include<stdio.h>
int main()
{
	int a;

	// Let we input 10 20, we get output as 20
	// (First input is ignored)
	// If we remove * from below line, we get 10.
	scanf("%*d%d", &a);

	printf( "%d ", a);

	return 0;	
}
