// C program to illustrate
// syntax error
#include<stdio.h>
void main()
{
	int x = 10;
	int y = 15;
	
	printf("%d", (x, y)) // semicolon missed
}








// C++ program to illustrate
// syntax error

#include <iostream>
using namespace std;

void main()
{
	int x = 10;
	int y = 15;
	
	cout << " "<< (x, y) // semicolon missed
}

// This code is contributed by shivanisinghss2110










// C program to illustrate
// syntax error
#include<stdio.h>
int main(void)
{
	// while() cannot contain "." as an argument.
	while(.)
	{
		printf("hello");
	}
	return 0;
}








// C++ program to illustrate
// syntax error
#include <iostream>
using namespace std;

int main(void)
{
	// while() cannot contain "." as an argument.
	while(.)
	{
		cout <<"hello";
	}
	return 0;
}

