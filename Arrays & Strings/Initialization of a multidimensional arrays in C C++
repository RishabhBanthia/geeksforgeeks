#include<bits/stdc++.h>
using namespace std;
int main()
{
int a[][][2] = { {{1, 2}, {3, 4}},
				{{5, 6}, {7, 8}} };
cout << sizeof(a);
getchar();
return 0;
}











#include<stdio.h>
int main()
{
int a[][][2] = { {{1, 2}, {3, 4}},
				{{5, 6}, {7, 8}}
				}; 
printf("%d", sizeof(a));
getchar();
return 0;
}








// Program 1
#include<stdio.h>
int main()
{
int a[][2] = {{1,2},{3,4}}; 
printf("%lu", sizeof(a));
getchar();
return 0;
}








// Program 1
#include<bits/stdc++.h>
using namespace std;
int main()
{
int a[][2] = {{1, 2}, {3, 4}}; 
cout << sizeof(a); 
return 0;
}










// Program 2
#include<stdio.h>
int main()
{
int a[][2][2] = { {{1, 2}, {3, 4}},
					{{5, 6}, {7, 8}}
				}; // Works
printf("%lu", sizeof(a)); // prints 8*sizeof(int)
getchar();
return 0;
}
