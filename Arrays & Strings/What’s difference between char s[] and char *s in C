#include <stdio.h>
int main()
{
	char s[] = "geeksquiz";
	printf("%lu", sizeof(s));
	s[0] = 'j';
	printf("\n%s", s);
	return 0;
}








#include <stdio.h>
int main()
{
	char *s = "geeksquiz";
	printf("%lu", sizeof(s));

	// Uncommenting below line would cause undefined behaviour
	// (Caused segmentation fault on gcc)
	// s[0] = 'j';
	return 0;
}
