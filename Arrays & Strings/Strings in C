#include<stdio.h>

int main()
{
	char str[] = "Geeks";
	
	// print string
	printf("%s",str);
	return 0;
}










#include<stdio.h>

int main()
{
	char str[50];
	scanf("%s",str);
	
	// print string
	printf("%s",str);
	return 0;
}







#include<stdio.h>

void printStr(char str[])
{
	printf("String is : %s",str);
}

int main()
{
	char str[] = "GeeksforGeeks";
	
	printStr(str);
	return 0;
}
