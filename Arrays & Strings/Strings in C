// C program to illustrate strings

#include<stdio.h>

int main()
{
	// declare and initialize string
	char str[] = "Geeks";
	
	// print string
	printf("%s",str);
	
	return 0;
}










// C program to read strings

#include<stdio.h>

int main()
{
	// declaring string
	char str[50];
	
	// reading string
	scanf("%s",str);
	
	// print string
	printf("%s",str);

	return 0;
}
