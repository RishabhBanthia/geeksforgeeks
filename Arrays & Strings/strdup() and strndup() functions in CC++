#include<stdio.h>
#include<string.h>

int main()
{
	char source[] = "GeeksForGeeks";

	char* target = strdup(source);

	printf("%s", target);
	return 0;
}










#include<stdio.h>
#include<string.h>

int main()
{
	char source[] = "GeeksForGeeks";

	char* target = strndup(source, 5);

	printf("%s", target);
	return 0;
}
