#include<stdio.h>
int main()
{
  char arr[] = "geeks";

  printf("%lu", sizeof(arr));

  return 0;
}








#include<stdio.h>
int main()
{
  char arr[5] = "geeks";
	
  printf("%lu", sizeof(arr));
	
  return 0;
}








#include<stdio.h>
int main()
{
// arr[] is not terminated with '\0'
// and its size is 5
char arr[]= {'g', 'e', 'e', 'k', 's'};

printf("%lu", sizeof(arr));

return 0;
}
