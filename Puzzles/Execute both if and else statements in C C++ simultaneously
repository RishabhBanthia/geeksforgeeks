#include <stdio.h>
int main()
{
    if (1)
    {
	label_1: printf("Hello ");

	goto label_2;
    }
    else
    {
	goto label_1;

	label_2: printf("Geeks");
    }
    return 0;
}

















#include <bits/stdc++.h>
using namespace std;
int main()
{
if (1)
{
	label_1: cout <<"Hello ";
	
	goto label_2;
}
else
{
	goto label_1;

	label_2: cout <<"Geeks";
}
return 0;
}
