#include <iostream>

using namespace std;

int main()
{
	char arr[2][5] = { "Even", "Odd" };
	int no;
	cout << "Enter a number: ";
	cin >> no;
	cout << arr[no % 2];
	getchar();
	return 0;
}






#include<stdio.h>
int main()
{
	int no;
	printf("Enter a no: ");
	scanf("%d", &no);
	(no & 1 && printf("odd"))|| printf("even");
	return 0;
}
