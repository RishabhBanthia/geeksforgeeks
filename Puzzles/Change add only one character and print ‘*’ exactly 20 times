#include <stdio.h>
int main()
{
	int i, n = 20;
	for (i = 0; i < n; n--)
	{
		printf("*");
	}
	getchar();
	return 0;
}















#include <iostream>
using namespace std;
int main()
{
	int i, n = 20;
	for (i = 0; i < n; n--)
	{
		cout << "*";
	}
	getchar();
	return 0;
}

















#include <stdio.h>
int main()
{
	int i, n = 20;
	for (i = 0; -i < n; i--)
	{
		printf("*");
	}
	getchar();
	return 0;
}

















#include<bits/stdc++.h>
using namespace std;

int main()
{
	int i, n = 20;
	for (i = 0; -i < n; i--)
		cout<<"*";		
	
	return 0;
}

// This code is contributed by rutvik_56.






#include <stdio.h>
int main()
{
	int i, n = 20;
	for (i = 0; i + n; i--)
	printf("*");
	getchar();
	return 0;
}






#include <iostream>
using namespace std;

int main()
{
	int i, n = 20;
	for(i = 0; i + n; i--)
		cout << "*";

	return 0;
}

