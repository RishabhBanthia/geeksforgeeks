int main(void)
{
	// Note that malloc() returns void * which can be
	// typecasted to any type like int *, char *, ..
	int *x = malloc(sizeof(int) * n);
}




#include<stdio.h>
int main()
{
	int a = 10;
	void *ptr = &a;
	printf("%d", *ptr);
	return 0;
}




#include<stdio.h>
int main()
{
	int a = 10;
	void *ptr = &a;
	printf("%d", *(int *)ptr);
	return 0;
}




#include<stdio.h>
int main()
{
	int a[2] = {1, 2};
	void *ptr = &a;
	ptr = ptr + sizeof(int);
	printf("%d", *(int *)ptr);
	return 0;
}
