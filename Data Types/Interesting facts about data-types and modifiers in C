#include <stdio.h>
int main()
{
	signed a;
	signed b;

	// size of a and b is equal to the size of int
	printf("The size of a is %d\n", sizeof(a));
	printf("The size of b is %d", sizeof(b));
	return (0);
}




#include <stdio.h>
int main()
{
	int x;
	char y;
	x = -1;
	y = -2;
	printf("x is %d and y is %d", x, y);
}




#include <stdio.h>
int main()
{
	signed float a;
	short float b;
	return (0);
}
