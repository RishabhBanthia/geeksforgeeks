#include <stdio.h>
int main()
{
	char a = 30, b = 40, c = 10;
	char d = (a * b) / c;
	printf ("%d ", d);
	return 0;
}




#include <stdio.h>

int main()
{
	char a = 0xfb;
	unsigned char b = 0xfb;

	printf("a = %c", a);
	printf("\nb = %c", b);

	if (a == b)
	printf("\nSame");
	else
	printf("\nNot Same");
	return 0;
}
