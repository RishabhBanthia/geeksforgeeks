#include <stdio.h>
int main(void)
{
    printf("%c\n", fun());
    return 0;
}
char fun()
{
   return 'G';
}













example:

#include <stdio.h>
 
char fun()
{
   return 'G';
}
int main(void)
{
    printf("%c\n", fun());
    return 0;
}














#include <stdio.h>
 
int fun()
{
   return 10;
}
 
int main(void)
{
    printf("%d\n", fun());
    return 0;
}


#include <stdio.h>
 
int main (void)
{
    printf("%d", sum(10, 5));
    return 0;
}
int sum (int b, int c, int a)
{
    return (a+b+c);
}
