#include <stdio.h>
int main(void)
{
    // Note that fun() is not declared
    printf("%c\n", fun());
    return 0;
}
 
char fun()
{
   return 'G';
}


example:

#include <stdio.h>
 
char fun()
{
   return 'G';
}
 
int main(void)
{
    // Note that fun() is not declared
    printf("%c\n", fun());
    return 0;
}


#include <stdio.h>
 
int fun()
{
   return 10;
}
 
int main(void)
{
    printf("%d\n", fun());
    return 0;
}
