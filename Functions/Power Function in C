#include <math.h>
#include <stdio.h>
  
int main()
{
    double x = 6.1, y = 4.8;

    double result = pow(x, y);
    printf("%.2lf", result);
  
    return 0;
}











Working of pow() function with integers

// C program to illustrate
// working with integers in
// power function
#include <math.h>
#include <stdio.h>
  
int main()
{
    int a;
  
    // Using typecasting for
    // integer result
    a = (int)(pow(5, 2) + 0.5);
    printf("%d", a);
  
    return 0;
}
