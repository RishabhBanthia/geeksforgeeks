#include <stdio.h>
#include <stdlib.h>

_Noreturn void view()
{
    return 10;
}
int main(void)
{
    printf("Ready to begin...\n");
    view();
  
    printf("NOT over till now\n");
    return 0;
}
















example:

#include <stdio.h>
#include <stdlib.h>

_Noreturn void show()
{
    printf("BYE BYE");
}
int main(void)
{
    printf("Ready to begin...\n");
    show();
  
    printf("NOT over till now\n");
    return 0;
}
