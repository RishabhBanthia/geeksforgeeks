#include <stdio.h>

void checkEvenOrNot(int num)
{
	if (num % 2 == 0)
	{
		goto even;
	
	]
	else
	{
		goto odd;
	}
even:
	printf("%d is even", num);
	return;
odd:
	printf("%d is odd", num);
}

int main() 
{
	int num = 26;
	checkEvenOrNot(num);
	return 0;
}











#include <iostream>
using namespace std;

void checkEvenOrNot(int num)
{
	if (num % 2 == 0)
	{
		goto even;
	}
	else
	{
		goto odd;
	}
even:
	cout << num << " is even";
	return;
odd:
	cout << num << " is odd";
}

int main()
{
	int num = 26;
	checkEvenOrNot(num);
	return 0;
}











#include <stdio.h>

void printNumbers()
{
	int n = 1;
label:
	printf("%d ",n);
	n++;
	if (n <= 10)
	{
		goto label;
	}
}

// Driver program to test above function
int main() 
{
	printNumbers();
	return 0;
}










#include <iostream>
using namespace std;

void printNumbers()
{
	int n = 1;
label:
	cout << n << " ";
	n++;
	if (n <= 10)
	{
		goto label;
	}
}

// Driver program to test above function
int main()
{
	printNumbers();
	return 0;
}
