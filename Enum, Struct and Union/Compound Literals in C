#include <stdio.h>
int main()
{
int *p = (int []){2, 4, 6};

printf("%d %d %d", p[0], p[1], p[2]);

return 0;
}











#include <stdio.h>

// Structure to represent a 2D point
struct Point
{
int x, y;
};

void printPoint(struct Point p)
{
printf("%d, %d", p.x, p.y);
}

int main()
{
printPoint((struct Point){2, 3});

return 0;
}
